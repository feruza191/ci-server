{
	"name": "ci-server",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "nodemon",
		"start-agent": "nodemon --exec ts-node --project ./tsconfig.json agent/index.ts",
		"start-server": "npm run build-server && nodemon dist/server/server.js",
		"start-dev": "nodemon --exec ts-node --project ./tsconfig.json server/src/server.ts",
		"build-client": "cross-env NODE_ENV=development webpack",
		"build-server": "cross-env TARGET=server NODE_ENV=development webpack",
		"eslint": "eslint client/**/* server/src/**/* --ext .js,.ts,.tsx --no-error-on-unmatched-pattern",
		"prettier": "prettier -c client/**/*.{ts,tsx} server/src/**/*.ts",
		"ts-check": "tsc -p ./tsconfig.json --noEmit"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
